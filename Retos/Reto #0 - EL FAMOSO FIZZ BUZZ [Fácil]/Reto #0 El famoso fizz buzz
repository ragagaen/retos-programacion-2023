current_number = 1
while current_number <= 100:
    print(current_number, "\n")
    current_number += 1
    
    
    
for current_number in range(1,101):
    if current_number % 3 == 0 and current_number % 5 == 0:
        print("fizzbuzz", "\n")
    elif current_number % 3 == 0:
        print("fizz", "\n")
    elif current_number % 5 == 0:
        print("buzz", "\n")
    else:
        print(current_number, "\n"
